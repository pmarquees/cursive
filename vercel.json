{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/chat/route.ts": { "maxDuration": 60 },
    "src/app/**/*.{js,ts,tsx}": { "maxDuration": 30 }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}